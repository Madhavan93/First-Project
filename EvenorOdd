package com.sample;
import java.util.Scanner;
public class Main{
  public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    int num = input.nextInt();
    for (int i=1;i<=num;i++)
      if (i%2==0){
        System,out.print(i);
        if (i!=num && i!=num-1){
          System.out.print(",");
        }
      }
    }
  }
}
