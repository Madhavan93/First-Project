package com.sample;
import java.util.Scanner;
public class Main{
  public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    int num = input.nextInt();
    if (num%2!=0){
      System.out.println("Given number is odd");
    } else {
      System.out.println("Given number is even");
    }
  }
}
